{
    "name": "openapi",
    "version": "0.1.0",
    "private": true,
    "scripts": {
      "generate:frontend": "openapi-generator-cli generate -i openapi.yaml -g typescript-axios -o frontend/axios",
      "generate:backend": "openapi-generator-cli generate -i openapi.yaml -g spring --library=spring-boot --api-package=fr.myguild.server.generated.api --model-package=fr.myguild.server.generated.model -o backend/java --additional-properties=interfaceOnly=true --additional-properties=dateLibrary=java8 --additional-properties=hideGenerationTimestamp=true",
      "generate": "npm run generate:frontend & npm run generate:backend",
      "delete": "rimraf backend/java & rimraf frontend/axios",
      "all": "npm ci && npm run delete && npm run generate"
    },
    "devDependencies": {
      "@openapitools/openapi-generator-cli": "^2.4.26",
      "rimraf": "^3.0.2"
    }
  }